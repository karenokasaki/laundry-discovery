{
   "version": 2,
   "outputDirectory": "dist",
   "builds": [
      {
         "src": "/dist/index.html",
         "use": "@vercel/static"
      },
      {
         "src": "/dist/assets/(.*)",
         "use": "@vercel/static"
      }
   ],
   "routes": [
      {
         "src": "/",
         "dest": "/dist/index.html"
      },
      {
         "src": "/(.*)",
         "dest": "/dist/$1"
      }
   ]
}
