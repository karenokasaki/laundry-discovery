{
   "version": 2,
   "outputDirectory": "dist",
   "builds": [
      {
         "src": "dist/**",
         "use": "@vercel/static"
      }
   ],
   "routes": [
      {
         "src": "/_assets/(.*)",
         "dest": "/_assets/$1"
      },
      {
         "src": "/(.*)",
         "dest": "/$1"
      }
   ]
}
