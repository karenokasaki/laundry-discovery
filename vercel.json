{
   "version": 2,
   "outputDirectory": "dist",
   "builds": [
      {
         "src": "/dist/index.html",
         "use": "@vercel/static"
      }
   ],
   "routes": [
      {
         "src": "/",
         "dest": "/dist/index.html"
      },
      {
         "src": "/(.*)",
         "dest": "/dist/$1"
      },
      {
         "src": "/assets/(.*)",
         "dest": "/dist/assets/$1",
         "headers": {
            "cache-control": "public, max-age=31536000, immutable"
         }
      }
   ]
}
